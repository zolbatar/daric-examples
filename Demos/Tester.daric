Def Pass()
    Ink 0,255,0
    Print "PASS!";
    Ink 255,255,255
    Print " (Test #";:Print testno;:Print ")"
    testno+=1
EndDef

Def Fail()
    Ink 0,255,0
    Print "!! FAILED TEST !! Quitting ";
    Ink 255,255,255
    Print " (Test #";:Print testno;:Print ")"
    GetChar
    End
EndDef

