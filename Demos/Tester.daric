    Dim a[10]
    For I=1 To 10
        a[I]=RND(1000)
    Next
    Sort a
    Print "Sort (Array)                 = ";:Pass()

    b=Vector of float
    For I=1 To 10
        a[I]=RNDF(1000.0)
    Next
    Sort b
    Print "Sort (Vector)                = ";:Pass()

    alr = vector OF someobj
    alr[1].aa="fgfdgd"
    alr[2].aa="r84jdjd"
    alr[3].aa="8564gdd"
    alr[4].aa="qrjshs"
    alr[5].aa="mbsgsy"
